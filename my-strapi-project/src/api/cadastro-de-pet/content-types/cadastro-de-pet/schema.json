{
  "kind": "collectionType",
  "collectionName": "cadastro_de_pets",
  "info": {
    "singularName": "cadastro-de-pet",
    "pluralName": "cadastro-de-pets",
    "displayName": "Pet",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "idade": {
      "type": "integer"
    },
    "foto": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "tamanho": {
      "type": "enumeration",
      "enum": [
        "Pequeno",
        "Médio",
        "Grande"
      ]
    },
    "sexo": {
      "type": "enumeration",
      "enum": [
        "Macho",
        "Fêmea"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "cadastro_de_pets"
    },
    "solicitacao_de_adocao": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::solicitacao-de-adocao.solicitacao-de-adocao",
      "mappedBy": "pet"
    },
    "raca": {
      "type": "string"
    },
    "nome": {
      "type": "string"
    },
    "descricao": {
      "type": "text"
    },
    "categoria": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::categoria.categoria",
      "inversedBy": "pets"
    }
  }
}
